{
  "name": "filter-lines-2",
  "displayName": "Filter Lines 2",
  "description": "Add regular expression matched text by filtering lines",
  "icon": "doc/logo.png",
  "version": "1.0.0",
  "publisher": "doctorgu",
  "repository": {
    "type": "git",
    "url": "https://github.com/doctorgu/vscode-filter-lines-2"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:filterlines.includeMatched",
    "onCommand:filterlines.includeMatchedGroup",
    "onCommand:filterlines.includeLine",
    "onCommand:filterlines.promptFilterLines",
    "onCommand:filterlines.filterLines"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "title": "Filter Lines: Include Matched",
        "command": "filterlines.includeMatched"
      },
      {
        "title": "Filter Lines: Include Matched Group",
        "command": "filterlines.includeMatchedGroup"
      },
      {
        "title": "Filter Lines: Include Line",
        "command": "filterlines.includeLine"
      }
    ],
    "keybindings": [
      {
        "command": "filterlines.includeMatched",
        "key": "ctrl-k ctrl-r",
        "mac": "cmd-k cmd-r"
      },
      {
        "command": "filterlines.includeMatchedGroup",
        "key": "ctrl-k ctrl-g",
        "mac": "cmd-k cmd-g"
      }
    ],
    "configuration": {
      "title": "Filter Lines",
      "properties": {
        "filterlines.caseSensitiveRegexSearch": {
          "type": "boolean",
          "default": true,
          "description": "If true, regex searches are case-sensitive"
        },
        "filterlines.preserveSearch": {
          "type": "boolean",
          "default": true,
          "description": "If true, your latest search string or regex will be saved, and then restored for your next search. If false, the search string or regex will be blank"
        },
        "filterlines.lineNumbers": {
          "type": "boolean",
          "default": false,
          "description": "Show source file line numbers in results"
        }
      }
    }
  },
  "scripts": {
    "clean": "rimraf out",
    "prebuild": "yarn run clean",
    "build": "tsc --noEmit -p . && babel src --out-dir out --extensions .js,.ts",
    "build:dev": "tsc --noEmit -p . && babel src --out-dir out --extensions .js,.ts --source-maps",
    "build:watch": "nodemon -e js,ts,json --watch src --watch .babelrc.js --exec \"babel src --out-dir out --extensions .js,.ts\"",
    "lint": "tslint -p .",
    "pretest": "set BABEL_ENV=coverage yarn build && yarn run coverage:clean",
    "test": "node ./out/test/runTest.js && yarn run nyc check-coverage",
    "posttest": "yarn run coverage:report",
    "coverage:clean": "rimraf .nyc_output coverage",
    "coverage:report": "nyc report --reporter=lcov --reporter=text-summary",
    "coverage:push": "nyc report --reporter=text-lcov | coveralls",
    "vscode:prepublish": "yarn run clean && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@doctorgu/tslint-rules": "doctorgu/tslint-rules#v1.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/sinon": "^10.0.12",
    "@types/vscode": "^1.24.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "assert": "^2.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "codecov": "^3.6.5",
    "coveralls": "^3.1.0",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.1",
    "nyc": "^15.1.0",
    "ovsx": "^0.1.0-next.a9154dc",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "test-exclude": "j03m/test-exclude#j03m/fix2",
    "tslint": "^5.19.0",
    "typescript": "^4.7.4",
    "vsce": "^1.87.0",
    "vscode-test": "^1.4.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/test/**/*.ts"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "checkCoverage": true,
    "branches": 80,
    "lines": 95,
    "functions": 95,
    "statements": 95
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.18.6"
  }
}
